{
	"name": "IcsTermine",
	"version": "1.0.1",
	"private": true,
	"scripts": {
		"clean:all": "npm i rimraf && rimraf node_modules .svelte-kit build KrankengymnastikTermine.ics",
		"clean:exe": "rimraf exe",
		"dev": "vite dev",
		"build": "vite build",
		"build:mac": "npm run clean:exe && npm run build && caxa --input . --output exe/IcsTermine.app --exclude \".git\" \".idea\" \"exe\" \"src\" \"static\" \".svelte-kit\" --uncompression-message \"First time startup may take some time, please wait...\" -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/server.mjs\"",
		"build:win": "npm run clean:exe && npm run build && caxa --input . --output exe/IcsTermine.exe --exclude \".git\" \".idea\" \"exe\" \"src\" \"static\" \".svelte-kit\" --uncompression-message \"First time startup may take some time, please wait...\" -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/server.mjs\"",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "node server.mjs",
		"docker:build": "docker build -t maugenst/icstermine .",
		"docker:push": "docker push maugenst/icstermine",
		"gcloud:deploy": "gcloud run deploy --source ."
	},
	"bin": "server.mjs",
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.4",
		"@types/config": "^3.3.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"caxa": "^3.0.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^2.1.1",
		"flowbite-svelte": "^0.44.19",
		"flowbite-svelte-icons": "^0.4.5",
		"nodemon": "^3.0.1",
		"postcss": "^8.4.31",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"svelty-picker": "^5.2.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@google-cloud/firestore": "^7.1.0",
		"@tinymce/tinymce-svelte": "^1.0.1",
		"body-parser": "^1.20.2",
		"config": "^3.3.9",
		"date-picker-svelte": "^2.10.0",
		"dayjs": "^1.11.10",
		"express": "^4.18.2",
		"firebase-admin": "^11.11.1",
		"fs-extra": "^11.2.0",
		"ics": "^3.5.0",
		"nodemailer": "^6.9.7",
		"open": "^9.1.0",
		"rimraf": "^5.0.5"
	}
}
